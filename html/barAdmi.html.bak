    function transferFct() {
	var y = bankinfo.query(client.eCardNumber);
	var a = JSON.parse(y);
	var pwd = $("#transferPassword").val();
	var ac = parseInt($("#transferAmount").val());
	var to = $("#transferTo").val();

	if(ac==""|| isNaN(ac) || ac<0){
		syalert.syopen("bankalert0")
	}
	else{
	    if(to == a['id']){
            syalert.syopen("bankalert1")
	    }
	    else {
            if (pwd == a['password']) {
                if (ac <= a['balance']) {
                    a['transferAmount'] = ac;
                    a['transferTo'] = to;

                    bankinfo.transfer(client.eCardNumber,to,ac);
                    syalert.syopen("bankalert2")
                } else {
                    syalert.syopen("bankalert3")
                }
            } else {
                syalert.syopen("bankalert4")
            }
        }
	}
    }

function topupFct(){
	var y = bankinfo.query(client.eCardNumber);
	var a = JSON.parse(y);
	var pwd = $("#topuppassword").val();
	var ac = parseInt($("#topupAmount").val());
    var m=$("input[name='toecard']:checked").val();
    
	if (ac==""|| isNaN(ac) || ac<0){
        syalert.syopen("bankalert0");
	}
	else {
		if(pwd == a['password']){
            if (m == 2) {
                if (ac <= a['balance']) {

                    bankinfo.transfToEcard(client.eCardNumber, ac);
                    syalert.syopen("bankalert5")
                }
                else {
                    syalert.syopen("bankalert3")
                }
            }
		    else if(m == 0){
		        if (ac <= a['eCardBalance']){
                    bankinfo.transfTocard(client.eCardNumber, ac);
                   syalert.syopen("bankalert6");
                }
		        else {
		            syalert.syopen("bankalert7");
                }
            }
		    else
		        syalert.syopen("bankalert8");
		}
		else{
			syalert.syopen("bankalert4");
		}   
	}
}


function searchFct(){

	var a = bankinfo.query(client.eCardNumber);
	var y = JSON.parse(a);
	var bal = y['balance'];
    var ebal = y['eCardBalance'];
    alert(bal);
    alert(ebal);
	var balRun = 0;
	var ebalRun = 0;
	var timesRun = 0;
	var stop = bal<ebal?ebal:bal;
	var myVar = setInterval(function(){
		if(balRun < bal)
			balRun += 1;
		if (ebalRun < ebal)
			ebalRun += 1;

		if(timesRun === stop){
			
			clearInterval(myVar);
		}
		//do whatever here..
		$("#balance").val(balRun);
		$("#Ecardbalance").val(ebalRun);
	}, 2);
}


